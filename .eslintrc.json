{
  "root": true,
  "extends": [
    "wordpress",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:jsx-a11y/recommended",
    "plugin:react-hooks/recommended",
    "plugin:prettier/recommended",
    "airbnb"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true,
      "arrowFunctions": true,
      "blockBindings": true,
      "classes": true,
      "defaultParams": true,
      "modules": true
    },
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "commonjs": true,
    "jquery": true
  },
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/no-array-index-key": "off",
    "react/jsx-props-no-spreading": "off",
    "react/react-in-jsx-scope": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "react/prop-types": "off",
    "import/prefer-default-export": "off",
    "prettier/prettier": [
      "error",
      {},
      {
        "usePrettierrc": true
      }
    ],
    "camelcase": [1],
    "spaced-comment": [0],
    "padded-blocks": [0],
    "prefer-template": [0],
    "max-len": [0],
    "no-else-return": [0],
    "func-names": [0],
    "object-shorthand": [0],
    "object-curly-newline": "off",
    "template-curly-spacing": "off",
    "indent": [
      "warn",
      2,
      {
        "ignoredNodes": ["TemplateLiteral"],
        "SwitchCase": 1
      }
    ],
    "space-before-function-paren": 0,
    "no-tabs": 0,
    "prefer-destructuring": 0,
    "no-undef": 0,
    "no-param-reassign": 0,
    "comma-dangle": 0,
    "lines-around-comment": 0
  }
}
