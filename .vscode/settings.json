{
  "workbench.colorCustomizations": {
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveForeground": "#e7e7e799",
    "titleBar.activeBackground": "#3c91a0",
    "titleBar.inactiveBackground": "#3c91a099",
    "activityBar.activeBackground": "#51aebe",
    "activityBar.activeBorder": "#f6e8f4",
    "activityBar.background": "#51aebe",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f6e8f4",
    "activityBarBadge.foreground": "#15202b",
    "editorGroup.border": "#51aebe",
    "panel.border": "#51aebe",
    "sash.hoverBorder": "#51aebe",
    "sideBar.border": "#51aebe",
    "statusBar.background": "#3c91a0",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#51aebe",
    "statusBarItem.remoteBackground": "#3c91a0",
    "statusBarItem.remoteForeground": "#e7e7e7"
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.formatOnSave": false
  },
  "eslint.alwaysShowStatus": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "prettier.disableLanguages": ["javascript", "javascriptreact"],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.excludeLanguages": ["markdown"],
  "peacock.color": "#3C91A0"
}
